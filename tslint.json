{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "./src/generated/**"
    ]
  },
  "jsRules": {},
  "rules": {
    "object-literal-sort-keys": false,
    "ordered-imports": [
      true,
      {
        "import-sources-order": "case-insensitive",
        "grouped-imports": true,
        "named-imports-order": "case-insensitive"
      }
    ]
  },
  "rulesDirectory": []
}
